<?xml version="1.0" encoding="iso-8859-1"?>
<root>
  <template name="resize_moon">
    <resize>
      <anchored>
        <right offset="-10" />
        <bottom offset="-10" />
      </anchored>
    </resize>
  </template>
  <template name="close_moon">
    <close>
      <anchored>
        <top offset="0" />
        <right offset="-8" />
      </anchored>
    </close>
  </template>
  <template name="list_moons">
    <windowlist>
      <script file="utility/scripts/moonlist.lua" />
      <datasource>.moons</datasource>
      <class>moonitem</class>
      <allowdelete />
    </windowlist>
  </template>
</root>
